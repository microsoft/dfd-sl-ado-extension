{
    "manifestVersion": 1,
    "id": "SlAdoFilterExtension",
    "publisher": "LaveeshRohra",
    "version": "1.0.1",
    "name": "Shift-Left ADO Filter Extension",
    "description": "Extension that uses docker filter command",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "categories": [
        "Azure Pipelines"
    ],
	"contributions": [
        {
            "id": "MSDSecurityStartDecorator",
            "type": "ms.azure-pipelines.pipeline-decorator",
            "targets": [
                "ms.azure-pipelines-agent-job.pre-job-tasks"
            ],
            "properties": {
                "template": "MDCDecorator/MDCDecoratorStart.yml"
            }
        },
		{
            "id": "MSDSecurityEndDecorator",
            "type": "ms.azure-pipelines.pipeline-decorator",
            "targets": [
                "ms.azure-pipelines-agent-job.post-job-tasks"
            ],
            "properties": {
                "template": "MDCDecorator/MDCDecoratorEnd.yml"
            }
        }
    ],
    "files": [
        {
            "path": "MDCDecorator/MDCDecoratorStart.yml",
            "addressable": true,
            "contentType": "text/plain"
        },
        {
            "path": "MDCDecorator/MDCDecoratorEnd.yml",
            "addressable": true,
            "contentType": "text/plain"
        }
    ]
}